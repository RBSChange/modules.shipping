<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action name="createFolder">
		<parameter name="listWidget"/>
		<body><![CDATA[
			this.createDocumentEditor('modules_media_folder', listWidget.getSelectedItems()[0].id);
		]]></body>
	</action>
	<action name="createMode">
		<parameter name="listWidget"/>
		<body><![CDATA[this.createDocumentEditor('modules_shipping_mode', listWidget.getSelectedItems()[0].id);]]></body>
	</action>
	
	<action name="createVirtualMode">
		<parameter name="listWidget"/>
		<body><![CDATA[this.createDocumentEditor('modules_shipping_virtualmode', listWidget.getSelectedItems()[0].id);]]></body>
	</action>
	
	<action name="openOrderMode">
		<parameter name="listWidget" />
		<body><![CDATA[
			this.openOrderChildrenPanel(listWidget, null, ['modules_shipping/mode','modules_generic/folder']);
		]]></body>
	</action>
</actions>
